{
    "name": "siel/acumulus-joomla",
    "description": "Integrates VirtueMart or HikaShop on Joomla with your Siel Acumulus online administration",
    "type": "joomla-package",
    "version": "8.3.0",
    "license": [
        "GPL-3.0"
    ],
    "require": {
        "php": "^8",
        "siel/acumulus": "^8.2.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Siel\\Joomla\\Component\\Acumulus\\Administrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siel\\Acumulus\\Tests\\": ["tests/", "vendor/siel/acumulus/tests/"],
            "Siel\\Acumulus\\TestWebShop\\": "vendor/siel/acumulus/tests/TestWebShop/"
        }
    }
}
